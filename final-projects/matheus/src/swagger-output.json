    "/register": {
      "post": {
        "tags": [
          "users"
        ],
        "description": "Route to create a new user",
        "parameters": [
          {
            "name": "obj",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "password": {
                  "example": "123456"
                },
                "firstName": {
                  "example": "Paketa"
                },
                "lastName": {
                  "example": "Academy"
                },
                "email": {
                  "example": "paketa.academy@gmail.com"
                }
              }
            }
          }
        ],
        "responses": {
          "422": {
            "description": "Sorry, this email is alredy registered",
            "content": "application/json"
          },
          "400": {
            "description": "Sorry, can't create this user",
            "content": "application/json"
          },
          "201": {
            "description": "User saved successfully",
            "content": "application/json"
          }
        }
      }
    },
    "/login": {
      "post": {
        "tags": [
          "users"
        ],
        "description": "Dedicated route to login a user",
        "parameters": [
          {
            "name": "obj",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "email": {
                  "example": "paketa.academy@gmail.com"
                },
                "password": {
                  "example": "123456"
                }
              }
            }
          }
        ],
        "responses": {
          "404": {
            "description": "Email not found / Wrong password",
            "content": "application/json"
          },
          "400": {
            "description": "Sorry, something went wrong",
            "content": "application/json"
          },
          "200": {
            "description": "Login successfully",
            "content": "application/json"
          }
        }
      }
    },
